{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
    "import/no-dynamic-require": 0,
    "global-require": 0,
    "no-unused-vars": 0,
    "camelcase": 0,
    "@typescript-eslint/no-var-requires": 0,
    "prettier/prettier": 0
    }
}
